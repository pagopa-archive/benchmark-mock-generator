  "devDependencies": {
    "@mockoon/cli": "^2.2.1",
    "@protocolnebula/ts-openapi-generator": "^2.6.0",
    "@stoplight/prism-cli": "^4.10.5",
    "typescript": "^4.9.4"
  }
}
